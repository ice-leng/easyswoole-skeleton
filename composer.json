{
  "name": "easyswoole-tool/skeleton",
  "description": "easyswoole skeleton common",
  "type": "library",
  "keywords": [
    "php",
    "easyswoole",
    "extension",
    "common"
  ],
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.2.0",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",

    "psr/container": "^1.0",
    "psr/simple-cache": "^1.0",
    "roave/better-reflection": "^4.0",
    "marc-mabe/php-enum": "^4.3",
    "swlib/saber": "^1.0",

    "easyswoole/easyswoole": "3.x",
    "easyswoole/jwt": "^1.1",
    "easyswoole/pay": "^1.2",
    "easyswoole/redis": "^1.3",
    "easyswoole/redis-pool": "^2.2",
    "easyswoole/http-annotation": "^2.0",
    "easyswoole/wechat": "^1.2",
    "easyswoole/oss": "^1.1",
    "alipaysdk/easysdk": "^2.2",
    "aliyuncs/oss-sdk-php": "^2.4",
    "alibabacloud/client": "^1.5",
    "qiniu/php-sdk": "^7.3",

    "easyswoole/http-annotation-swagger": "dev-master",
    "easyswoole/hyperf-orm": "dev-master"
  },
  "require-dev": {
    "swoole/ide-helper": "~4.4.7"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "EasySwoole\\Skeleton\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "authors": [
    {
      "name": "lengbin",
      "email": "xykxyk2008@163.com"
    }
  ],
  "extra": {
    "easyswoole": {
      "config": "EasySwoole\\Skeleton\\ConfigProvider"
    }
  }
}
